#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/js/bin/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/js/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules:/Users/huangqiang/projects/node_modules:/Users/huangqiang/node_modules:/Users/node_modules:/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/js/bin/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/js/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/js-beautify/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules:/Users/huangqiang/projects/node_modules:/Users/huangqiang/node_modules:/Users/node_modules:/node_modules:/Users/huangqiang/projects/tt-note-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-beautify/js/bin/css-beautify.js" "$@"
else
  exec node  "$basedir/../js-beautify/js/bin/css-beautify.js" "$@"
fi
